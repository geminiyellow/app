{
  "name": "meth-browser",
  "version": "1.0.0",
  "description": "Meth Dapp browser",
  "main": "electron/index.js",
  "scripts": {
    "electron": "electron ./electron",
    "web": "webpack --env.development --env.vendor && webpack-dev-server --env.development",
    "web:prod": "webpack --env.production --env.vendor && webpack --env.production --progress",
    "web:serve": "http-serve -p 3001 --gzip true ./build/web",
    "precommit": "npm run prettier && npm run lint:js:fix",
    "prettier": "prettier --config ./.prettierrc --write src/**/*.js",
    "lint:js": "eslint src/",
    "lint:js:fix": "eslint --fix src/",
    "test": "jest",
    "build": "npm run lint:js && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meth-project/meth-browser.git"
  },
  "keywords": [
    "ethereum",
    "solidity",
    "wallet",
    "dapp",
    "browser",
    "react",
    "react-native",
    "contracts",
    "blockchain",
    "electron",
    "electronjs",
    "geth",
    "parity"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
