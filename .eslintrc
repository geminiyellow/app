{
  "extends": "standard",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "jest": true
  },
  "plugins": [ "react" ],
  "rules": {
    "generator-star-spacing": ["error", {"before": true, "after": false}],
    "no-multiple-empty-lines": 0,
    "yoda": 0,
    "comma-dangle": 0,
    "one-var": 0,
    "brace-style": 0,
    "react/jsx-uses-vars": 2,
    "react/jsx-uses-react": 2
  },
  "globals": {
    "__DEV__": true,
    "window": true,
    "fetch": true
  }
}
